{
    "Format": "2.0.0",
	
//	CONFIG
	
    "ConfigSchema": {
		"Use_Waterfall_Cave_WaFF": {
			"AllowValues": "False, True",
			"Default": "True",
			"Description": "Make the Waterfall Cave accessible on your farm (by default, this will be in addition to the normal cave). Disabling this option will remove all of the cave extra entrances from the map.",
			"Section": "Cave_Shortcuts_WaFF"			
		},
		"Tent_Entrance_WaFF": {
			"AllowValues": "False, True",
			"Default": "False",
			"Description": "Replace the cave entrance between the farmhouse and the bus exit with a tent, similar to older versions of the mod.",
			"Section": "Cave_Shortcuts_WaFF"
		},
		"Cellar_Allows_Casks_in_Cave_WaFF": {
			"AllowValues": "False, True",
			"Default": "False",
			"Description": "Once the farmhouse cellar is built, extend the usage of casks to the Waterfall Cave.",
			"Section": "Cave_Shortcuts_WaFF"
        },
		"Smaller_Shortcut_Caves_WaFF": {
			"AllowValues": "False, True",
			"Default": "False",
			"Description": "Make the shortcuts' entrances smaller on the farm. Beware that regular horses will no longer be able to use them!",
			"Section": "Cave_Shortcuts_WaFF"
		},
		"Secret_Woods_Shortcut_WaFF": {
			"AllowValues": "False, True",
			"Default": "True",
			"Description": "Unlock a path next to the farm's shrine that will quickly lead to the Secret Woods if the player has already been there at least once.",
			"Section": "Cave_Shortcuts_WaFF"
        },
//		
		"Use_Animal_Pen_WaFF": {
			"AllowValues": "False, True",
			"Default": "False",
			"Description": "Place one or more animal pens on the western side of the farm.",
			"Section": "Animals_Monsters_WaFF"
		},	
		"Animal_Pen_Size_WaFF": {
			"AllowValues": "2_Med, 1_Med, 1_Large",
			"Default": "2_Med",
			"Description": "Select the amount and size of the animal pens.",
			"Section": "Animals_Monsters_WaFF"
		},		
		"Use_Invisible_Barriers_WaFF": {
			"AllowValues": "False, True",
			"Default": "True",
			"Description": "Prevent animals from using stairs, bridges or leaving the pen. Disable to let them roam the farm freely.",
			"Section": "Animals_Monsters_WaFF"
		},
//
		"Beach_Type_WaFF": {
			"AllowValues": "Sea, Lake, Dry, Dry_Pond",
			"Default": "Sea",
			"Description": "Choose between a dry beach full of sand with very few water, a grassy beach with a large lake or a saltwater one with sea forage and palm trees.",
			"Section": "Water_Features_WaFF"
		},
		"Beach_Bridge_WaFF": {
			"AllowValues": "Sand, Planks",
			"Default": "Sand",
			"Description": "Turn the small bridges on the beach into either wooden planks or natural sand. Beware that the sand option will prevent you from laying path tiles over it, which may hinder mods relying on said tiles to work (like Automate).",
			"Section": "Water_Features_WaFF"
		},		
		"Fish_Pool_WaFF": {
			"AllowValues": "Forest, Sea",
			"Default": "Forest",
			"Description": "Select the type of fishes that can be caught on the farm (this feature will change after Stardew Valley 1.6 update). Sea fishes can only be caught if the sea beach is active.",
			"Section": "Water_Features_WaFF"
		},		
		"Turn_Pond_into_Spa_WaFF": {
			"AllowValues": "False, True",
			"Default": "True",
			"Description": "Turn the small pond south of the twin waterfalls into a healing pond.",
			"Section": "Water_Features_WaFF"
		},
		"No_Water_Spa_WaFF": {
			"AllowValues": "False, True",
			"Default": "False",
			"Description": "Remove water effects and properties from the healing pond (usefull for some mod compatibility issues, note that this is automatically actived when using the Swim mod).",
			"Section": "Water_Features_WaFF"
		},
		"Fountain_Decoration_WaFF": {
			"AllowValues": "False, True",
			"Default": "True",
			"Description": "Add a fountain and its decorated surroundings to the farm.",
			"Section": "Water_Features_WaFF"
		},
//	
		"TF_Dirt_Color_WaFF": {
			"AllowValues": "Automatic, Dark, Light",
			"Default": "Automatic",
			"Description": "Select the color of the dirt around the twin waterfalls area. The automatic option will leave the dirt dark and only change it in case of compatibility issues (like mods trying to use said dirt as quarry tiles).",
			"Section": "Natural_Terrain_Features_WaFF"
		},
		"Add_Cliffs_N_WaFF": {
			"AllowValues": "False, True",
			"Default": "True",
			"Description": "Add a cliff on the farmhouse main island.",
			"Section": "Natural_Terrain_Features_WaFF"
		},
		"Add_Cliffs_E_WaFF": {
			"AllowValues": "False, True",
			"Default": "True",
			"Description": "Add a cliff to the eastern part of the map.",
			"Section": "Natural_Terrain_Features_WaFF"
		},
		"Add_Small_Ridges_N_WaFF": {
			"AllowValues": "False, True",
			"Default": "False",
			"Description": "Add a small ridge south of the farmhouse.",
			"Section": "Natural_Terrain_Features_WaFF"
		},
		"Add_Small_Ridges_E_WaFF": {
			"AllowValues": "False, True",
			"Default": "False",
			"Description": "Add a small ridge near the south-eastern bridge.",
			"Section": "Natural_Terrain_Features_WaFF"
		},
		"Dense_Forest_Area_WaFF": {
			"AllowValues": "False, True",
			"Default": "False",
			"Description": "Add trees and a dense foliage above the tree stumps West of the farm.",
			"Section": "Natural_Terrain_Features_WaFF"
		},		
//
		"Fences_WaFF": {
			"AllowValues": "Standard, Less, More, Even_More",
			"Default": "Standard",
			"Description": "Handle the amount of decorative fences that will be present on the farm.",
			"Section": "Artificial_Terrain_Features_WaFF"
		},	
		"Small_Stairs_Hills_WaFF": {
			"AllowValues": "False, True",
			"Default": "False",
			"Description": "Reverse the two hills' large stairs at the eastern side of the map into their original smaller size. Beware that regular horses will no longer be able to use them!",
			"Section": "Artificial_Terrain_Features_WaFF"
		},
		"Old_Main_Bridge_WaFF": {
			"AllowValues": "False, True",
			"Default": "False",
			"Description": "Replace the bridge beween the farmhouse and the path to the bus stop by the bigger one used in previous versions of the mod.",
			"Section": "Artificial_Terrain_Features_WaFF"
		}
    },

